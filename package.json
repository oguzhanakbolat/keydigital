    "expo-router": "~6.0.21",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-text-extractor": "^0.2.2",
    "expo-web-browser": "~15.0.10",
    "formik": "^2.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
