{
  "name": "lock-smith",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android --device",
    "ios": "OS_ACTIVITY_MODE=disable expo run:ios --device",
    "ios:sim": "OS_ACTIVITY_MODE=disable expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "build": "eas build --platform all --profile production",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "prebuild": "npx expo prebuild --clean",
    "local:android": "eas build --platform android --local",
    "local:apk": "eas build --platform android --local --profile apk",
    "local:ios": "eas build --platform ios --local --profile production --clear-cache"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.4.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.13.1",
    "expo": "~54.0.31",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.13",
    "expo-document-picker": "~14.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
